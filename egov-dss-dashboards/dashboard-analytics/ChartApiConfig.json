  "DSSiLMStotalCase": {
    "chartName": "DSS_TOTAL_CASES",
    "queries": [
      {
        "module": "iLMS",
        "dateRefField": "Data.registrationDate",
        "requestQueryMap": "{\"state\" : \"Data.court.state.keyword\", \"district\" : \"Data.court.district.keyword\",\"region\" : \"Data.court.division.keyword\", \"tenantId\" : \"Data.tenantId.keyword\"}",
